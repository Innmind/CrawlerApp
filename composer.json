{
    "name": "innmind/crawler-app",
    "license": "proprietary",
    "description": "Crawl the web and publish the graph to an api",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Crawler\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Crawler\\": "tests/"
        }
    },
    "require": {
        "php": "~7.4|~8.0",
        "innmind/crawler": "~6.0",
        "innmind/robots-txt": "~5.0",
        "innmind/operating-system": "~2.0",
        "innmind/amqp": "~3.0",
        "innmind/cli": "~2.0",
        "monolog/monolog": "~2.0",
        "symfony/dotenv": "~5.0",
        "innmind/logger": "~2.0",
        "innmind/rest-client": "~8.0",
        "innmind/homeostasis": "~4.0",
        "innmind/installation-monitor": "~3.0",
        "innmind/json": "^1.1",
        "innmind/silent-cartographer": "~2.0",
        "innmind/stack": "^1.0",
        "innmind/ipc": "~3.0",
        "innmind/genome": "^3.0",
        "innmind/cli-framework": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "roave/security-advisories": "dev-master",
        "innmind/debug": "~2.0",
        "vimeo/psalm": "~4.0",
        "innmind/black-box": "^4.17",
        "innmind/coding-standard": "^1.1"
    },
    "provide": {
        "innmind/genome-genes": "3.0"
    }
}
